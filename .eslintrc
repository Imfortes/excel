{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "linebreak-style": "off",
    "indent": "off",
    "comma-dangle": "off",
    "padded-blocks": "off",
    "no-useless-escape": "off",
    "no-trailing-spaces": "off",
    "eol-last": "off",
    "require-jsdoc": "off",
    "operator-linebreak": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
